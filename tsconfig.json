{
    "compilerOptions": {
        "module": "commonjs",
        "target": "es6",
        "sourceMap": true,
        "baseUrl": "./src",
        "paths": {
            "*": ["*"]
          },
        "removeComments": false,
        "noUnusedParameters": true
    },
    "include": [
        "src/**/*"
    ],
    "exclude": [
        "node_modules"
    ]
}
